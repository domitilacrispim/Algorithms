#include <stdio.h>
int main (){
	printf("Enter the number of elements:\n");
	int a, k[1000],aux;
	scanf ("%d", &a);
	printf("Enter the elements:\n");
	for ( int i=0; i<a; i++){
		scanf ("%d", &k[i]);
	}
	for ( int t=0; t<a; t++){
		for (int r=0; r<a; r++){
			if(k[t]<k[r]){
				aux=k[t];
				k[t]=k[r];
				k[r]=aux;
			}
		}
	}
	printf ("Orded using Bubble Sort\n");
	for ( int y=0; y<a; y++){
		printf ("%d ", k[y]);
	}
}
